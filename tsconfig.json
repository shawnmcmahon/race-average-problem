{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "es5",
      "dom",
      "dom.iterable",
      "esnext",
    ],
    "types": ["cypress"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src", "**/*.ts"
  ]
}
